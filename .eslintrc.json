{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "camelcase": 0,
    "max-len": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0
  }
}
